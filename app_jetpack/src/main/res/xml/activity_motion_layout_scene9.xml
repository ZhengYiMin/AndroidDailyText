<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintHorizontal_bias="0.141" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.141" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.141" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView4"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.141" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.141" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.87"
            motion:layout_constraintHorizontal_bias="0.141"
            motion:layout_constraintEnd_toEndOf="parent"
            android:rotation="360"
            />
        <Constraint
            motion:layout_editor_absoluteY="522dp"
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintCircle="@id/imageView"
            motion:layout_constraintCircleRadius="180dp"
            motion:layout_constraintCircleAngle="0"
            android:layout_width="50dp"
            android:rotation="360"
            android:id="@+id/imageView3" />
        <Constraint
            motion:layout_editor_absoluteY="543dp"
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintCircle="@id/imageView"
            motion:layout_constraintCircleRadius="180dp"
            motion:layout_constraintCircleAngle="30"
            android:layout_width="50dp"
            android:rotation="360"
            android:id="@+id/imageView4" />
        <Constraint
            motion:layout_editor_absoluteY="651dp"
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintCircle="@id/imageView"
            motion:layout_constraintCircleRadius="180dp"
            motion:layout_constraintCircleAngle="60"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:rotation="360"
            android:id="@+id/imageView2" />
        <Constraint
            motion:layout_editor_absoluteY="523dp"
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintCircle="@id/imageView"
            motion:layout_constraintCircleRadius="180dp"
            motion:layout_constraintCircleAngle="90"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:rotation="360"
            android:id="@+id/imageView5" />
    </ConstraintSet>
</MotionScene>